upper_border_first_num = int(input())
upper_border_second_num = int(input())
upper_border_third_num = int(input())


for num1 in range(1, upper_border_first_num + 1):
    if num1 % 2 == 0:

        for num2 in range(2, upper_border_second_num + 1):

            for num3 in range(1, upper_border_third_num + 1):
                if num3 % 2 == 0:

                    if num2 == 2 or num2 == 3 or num2 == 5 or num2 == 7:
                        print(f'{num1} {num2} {num3}')
